{
  "name": "frontend",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run set-version && ng serve",
    "build": "npm run set-version && ng build",
    "build:prod": "npm run set-version && ng build --configuration production",
    "set-version": "node scripts/set-version.js",
    "check-version": "node scripts/set-version.js && cat src/assets/runtime-config.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docker:build": "npm run set-version && node scripts/docker-build-local.js",
    "docker:run": "docker run -p 8080:80 frontend-local",
    "local:build": "powershell -ExecutionPolicy Bypass -File scripts/local-build.ps1"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.3",
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/material": "^20.2.1",
    "@angular/platform-browser": "^20.2.3",
    "@angular/router": "^20.2.3",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "bootstrap": "^5.3.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "@types/jasmine": "~5.1.0",
    "dotenv": "^17.2.3",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
